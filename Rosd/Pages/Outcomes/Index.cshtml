@page
@model Rosd.Pages.Outcomes.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].AttnDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].DueDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Way)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].RepTo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Subject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Person)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outcome[0].Note)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Outcome) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AttnDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DueDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Way)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RepTo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Note)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
